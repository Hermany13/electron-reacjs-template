{
  "name": "template-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "24.0.21",
    "@types/node": "12.12.3",
    "@types/react": "16.9.11",
    "@types/react-dom": "16.9.3",
    "dotenv": "^8.2.0",
    "electron": "^7.0.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "3.2.0",
    "typescript": "3.6.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently --names 'tsc,react' --prefix name 'yarn dev:tsc' 'yarn dev:react'",
    "dev:tsc": "tsc-watch ./src/browser/* --outDir ./dist/browser --onSuccess 'yarn dev:electron'",
    "dev:react": "cross-env BROWSER=none yarn start",
    "dev:electron": "wait-on http://localhost:3000 && node scripts/start-dev.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "electron-builder": "^22.1.0",
    "electron-devtools-installer": "^2.2.4",
    "tsc-watch": "^4.0.0",
    "wait-on": "^3.3.0"
  }
}